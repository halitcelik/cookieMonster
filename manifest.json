{

  "manifest_version": 2,
  "name": "cookieMonster",
  "version": "1.0",

  "description": "Displays cookies used in a page",

  "icons": {
    "48": "icons/borderify.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cookie.js"]
    }
  ],

  "permissions": [
    "webRequest"
  ]

}
